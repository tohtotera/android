<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tohtotera.ab.auonenet.jp.widgetintroduction">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ClockActivity"></activity>
        <activity android:name="ButtonActivity"></activity>
        <activity android:name="EditActivity"></activity>
        <activity android:name="MiscActivity"></activity>
        <activity android:name="ChronometerActivity"></activity>
        <activity android:name="DateTimePickerActivity"></activity>
        <activity android:name="GalleryActivity"></activity>
        <activity android:name="ProgressActivity"></activity>
        <activity android:name="VideoActivity"></activity>
        <activity android:name="PopupActivity"></activity>
    </application>

</manifest>
